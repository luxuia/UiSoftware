<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uisoftware"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainView"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ConnectView"
            android:label="@string/title_activity_connect_view" >
          
        </activity>
        <activity
            android:name=".QueryView"
            android:label="@string/title_activity_query_view" >
           
        </activity>
        <activity
            android:name=".QuerySetView"
            android:label="@string/title_activity_history_view" >
          
        </activity>
        <activity
            android:name=".HomeView"
            android:label="@string/title_activity_home_view" >
         
        </activity>
    </application>

</manifest>